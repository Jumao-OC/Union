<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zj.union.mapper.TFeedbackMapper">

    <insert id="insertFeedback" parameterType="com.zj.union.entity.Feedback">
        INSERT INTO t_feedback (fbtype, title,content,email)
        VALUES (#{fbtype},#{title},#{content},#{email});

    </insert>
</mapper>
